      "settings": {
        "foreground": "#bcb28d"
      }
    },
    {
      "scope": [
        "support.class",
        "variable",
        "entity.name.type",
        "entity.other.inherited-class",
        "keyword.operator",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#E6844F"
      }
    },
    {
      "scope": ["string", "string.regexp", "keyword.operator"],
      "settings": {
        "foreground": "#66A866"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#58b2dc"
      }
    },
    {
      "scope": [
        "entity.name.type.module.js",
        "entity.name.type.module.js.jsx",
        "entity.other.inherited-class.js",
        "entity.other.inherited-class.js.jsx"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#E03C8A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#DDA52D"
      }
    },
    {
      "scope": ["markup.underline.link.markdown"],
      "settings": {
        "foreground": "#58b2dc"
      }
    },
    {
      "scope": [
        "meta.separator.markdown",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#DDA52D"
      }
