    "list.activeSelectionForeground": "#bcb28d",
    "list.focusForeground": "#bcb28d",
    "list.inactiveSelectionBackground": "#243d46",
    "list.inactiveSelectionForeground": "#bcb28d",
    "list.warningForeground": "#e6844f",
    "list.errorForeground": "#E03C8A",
